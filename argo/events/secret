###  CREATE SECTET USING BELOW COMMAND 

kubectl create secret generic docker-credentials \
  --namespace argo \
  --from-literal=username=raju.tiwari@intuji.com \
  --from-literal=password=H#Qfi4gY8meL&Gq$

kubectl create secret generic slack-webhook \
  --namespace argo \
  --from-literal=url=https://hooks.slack.com/services/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